#include <stdio.h>
#include <math.h>
int main()
{
float x1, y1, x2, y2, x3, y3, a, b, c;
printf("Input point A coordinates in decart system:\n");
scanf("%f %f", &x1, &y1);
printf("Input point B coordinates in decart system:\n");
scanf("%f %f", &x2, &y2);
printf("Input point C coordinates in decart system:\n");
scanf("%f %f", &x3, &y3);
a=(x1*x1)+(y1*y1);
b=(x2*x2)+(y2*y2);
c=(x3*x3)+(y3*y3);
if (a<b && a<c)
{
    printf("point A is the closest to position (0, 0) ");
}
if (b<a && b<c)
{
    printf("point B is the closest to position (0, 0) ");
}
if (c<b && c<a)
{
    printf("point C is the closest to position (0, 0). ");
}
if (a==b && a==c)
{
    printf("points are at same position");
}
return 0;
}
